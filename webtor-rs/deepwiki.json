{
  "name": "webtor-rs",
  "description": "Browser-focused Tor client in Rust/WASM with Snowflake and WebTunnel transports",
  "include": [
    "webtor/**/*.rs",
    "webtor-wasm/**/*.rs",
    "webtor-demo/**/*.rs",
    "subtle-tls/**/*.rs",
    "README.md",
    "CHANGELOG.md",
    "docs/**/*.md"
  ],
  "exclude": [
    "target/**",
    "vendor/**",
    "node_modules/**"
  ]
}
